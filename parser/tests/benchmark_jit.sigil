// JIT-Compatible Benchmark - Pure recursive computation

fn fib(n) {
    if n <= 1 {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}

fn ackermann(m, n) {
    if m == 0 {
        return n + 1;
    }
    if n == 0 {
        return ackermann(m - 1, 1);
    }
    return ackermann(m - 1, ackermann(m, n - 1));
}

fn tak(x, y, z) {
    if y >= x {
        return z;
    }
    return tak(tak(x - 1, y, z), tak(y - 1, z, x), tak(z - 1, x, y));
}

fn main() {
    let r1 = fib(35);
    let r2 = ackermann(3, 7);
    let r3 = tak(18, 12, 6);
    return r1 + r2 + r3;
}
